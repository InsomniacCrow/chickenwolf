{
  "name": "chickenwolf",
  "version": "1.0.0",
  "description": "testing bot",
  "homepage": "https://github.com/InsomniacCrow/chickenwolf#readme",
  "bugs": {
    "url": "https://github.com/InsomniacCrow/chickenwolf/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InsomniacCrow/chickenwolf.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.ts",
    "build": "tsup src/index.ts --minify",
    "register": "tsx watch src/deploy-commands.ts"
  },
  "dependencies": {
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "keyv": "^5.5.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "removeComments": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
